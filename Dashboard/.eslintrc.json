{
    "env": {
        "browser": true,
        "es2021": true,
        "node": true
    },
    "extends": [
        "plugin:react/recommended",
        "standard-with-typescript"
    ],
    "overrides": [
    ],
    "parserOptions": {
        "ecmaVersion": "latest",
        "sourceType": "module"
    },
    "plugins": [
        "react",
        "only-warn"
    ],
    "rules": {
        "space-before-function-paren": ["error", "never"],
        "semi": 0,
        "no-multiple-empty-lines": 0,
        "no-trailing-spaces": 0
    }
}
